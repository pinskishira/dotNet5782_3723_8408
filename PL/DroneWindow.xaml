<Window x:Class="PL.DroneWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PL"
        mc:Ignorable="d"
        Title="DroneWindow" Height="450" Width="800">
    <Grid>
        <Label Content="ID" HorizontalAlignment="Left" Margin="31,36,0,0" VerticalAlignment="Top" RenderTransformOrigin="-4.446,-1.449" Height="27" Width="27"/>
        <Label Content="Model" HorizontalAlignment="Left" Margin="31,88,0,0" VerticalAlignment="Top" Width="71"/>
        <Label Content="Weight" HorizontalAlignment="Left" Margin="31,136,0,0" VerticalAlignment="Top"/>
        <ComboBox x:Name="WeightLabel" HorizontalAlignment="Left" Margin="251,136,0,0" VerticalAlignment="Top" Width="113" SelectionChanged="WeightLabel_SelectionChanged"/>
        <Label Content="Battery" HorizontalAlignment="Left" Margin="31,191,0,0" VerticalAlignment="Top"/>
        <Label Content="Status" HorizontalAlignment="Left" Margin="31,255,0,0" VerticalAlignment="Top"/>
        <Label Content="Location" HorizontalAlignment="Left" Margin="31,323,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.494,0.348"/>
        <Label Content="Parcel in Transfer" HorizontalAlignment="Left" Margin="31,393,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="ModelLabel" HorizontalAlignment="Left" Margin="251,88,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="113" Height="22" TextChanged="ModelLabel_TextChanged"/>
        <TextBox x:Name="IDLabel" HorizontalAlignment="Left" Margin="251,41,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="113" Height="22" TextChanged="IDLabel_TextChanged"/>
        <TextBox x:Name="BatteryLabel" HorizontalAlignment="Left" Margin="251,204,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="113" Height="21" IsReadOnly="True" TextChanged="BatteryLabel_TextChanged"/>
        <TextBox x:Name="StatusLabel" HorizontalAlignment="Left" Margin="251,255,0,0" Text="Maintanace" TextWrapping="Wrap" VerticalAlignment="Top" Width="113" Height="31"/>
        <Label Content="Latitude" HorizontalAlignment="Left" Margin="128,313,0,0" VerticalAlignment="Top" Width="73" Height="23"/>
        <Label Content="Longitutde" HorizontalAlignment="Left" Margin="128,341,0,0" VerticalAlignment="Top"/>
        <TextBox HorizontalAlignment="Left" Margin="251,313,0,0" Text="" TextWrapping="Wrap" VerticalAlignment="Top" Width="113" Height="23"/>
        <TextBox HorizontalAlignment="Left" Margin="251,341,0,0" Text="" TextWrapping="Wrap" VerticalAlignment="Top" Width="113" Height="26"/>
    </Grid>
</Window>
